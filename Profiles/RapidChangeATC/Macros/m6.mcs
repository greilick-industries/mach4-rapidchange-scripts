function m6()
  -- rcSub.Validate_HomeXYZ()
  local runToolChange = rcSub.Validate_m6()

  if (runToolChange == 0) then
    do return end
  end

  rcSub.Setup_m6()
  rcSub.UnloadTool()
  rcSub.LoadTool()
  rcSub.Execute_ToolTouchOff()
  rcSub.Teardown_m6()
end

if (mc.mcInEditor() == 1) then
	local inst = mc.mcGetInstance()
	local profile = mc.mcProfileGetName(inst)
	dofile ("Profiles\\" .. profile .. "\\Macros\\load_rapidChangeATC.mcs")
	m6()
end
